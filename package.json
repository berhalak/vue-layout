{
	"name": "@berhalak/vue-layout",
	"version": "0.1.0",
	"private": false,
	"description": "vue layout components",
	"main": "dist/vue-layout.umd.js",
	"types": "src/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/berhalak/vue-layout.git"
	},
	"keywords": [
		"vue",
